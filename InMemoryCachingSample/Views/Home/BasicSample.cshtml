@{
    ViewData["Title"] = "Basic Example";
    @model User;
}

<div class="text-center">
    <h2 class="display-5">Basic In-Memory Example</h2>
    
    <h4>Cached User: @(Model == null ? "No cached entry found" : Model.email)</h4>
    
    <a class="btn btn-primary" asp-action="CacheUser">Cache It</a>
    <a class="btn btn-secondary" asp-action="CacheRemove">Clear It</a>

</div>
